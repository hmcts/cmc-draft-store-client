{
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": false,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "lib": [
      "es6",
      "es2017.object",
      "dom"
    ],
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ]
  },
  "exclude": [
    "node_modules",
    "src/test",
    "types"
  ]
}
